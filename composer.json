{
    "name": "pmg/sqs-transport",
    "description": "A Symfony Messenger transport for SQS",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "PMG",
            "email": "tech@pmg.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "symfony/messenger": "^4.4 || ^5.4",
        "aws/aws-sdk-php": "^3.112"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/serializer": "^4.4 || ^5.4",
        "symfony/property-access": "^4.4 || ^5.4",
        "symfony/framework-bundle": "^4.4 || ^5.4",
        "symfony/phpunit-bridge": "^4.4 || ^5.4",
        "aws/aws-sdk-php-symfony": "^2.0",
        "symfony/yaml": "^4.4 || ^5.4"
    },
    "autoload": {
        "psr-4": {
            "PMG\\SqsTransport\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PMG\\SqsTransport\\Test\\": "test/"
        }
    }
}
